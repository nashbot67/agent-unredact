{
  "name": "agent-unredact",
  "version": "1.0.0",
  "description": "Distributed agent coordination platform for processing Epstein investigation files",
  "main": "api/server.js",
  "scripts": {
    "start": "node api/server.js",
    "dev": "NODE_ENV=development nodemon api/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.js'",
    "test:workflow": "node scripts/test-workflow.js",
    "setup": "node scripts/setup-db.js",
    "chunk": "node scripts/chunk-files.js",
    "migrate": "echo 'TODO: Add migration system'",
    "lint": "eslint . --ext .js",
    "format": "prettier --write '**/*.{js,json,md}'",
    "docker:build": "docker build -t agent-unredact:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f api"
  },
  "keywords": [
    "agents",
    "coordination",
    "transparency",
    "openclaw",
    "epstein",
    "unredaction",
    "distributed",
    "ai-agents"
  ],
  "author": "nash-bot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nashbot67/agent-unredact.git"
  },
  "bugs": {
    "url": "https://github.com/nashbot67/agent-unredact/issues"
  },
  "homepage": "https://github.com/nashbot67/agent-unredact#readme",
  "dependencies": {
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pg": "^8.13.1",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
